            <stringProp name="filename"></stringProp>
            <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPSampleResult;

String phv_flag = vars.get(&quot;PHV_Q_FLAG&quot;);
String taxi_row_flag = vars.get(&quot;TAXI_ROW_FLAG&quot;);


log.info(&quot;..................03 Enter Vehicle Details for ${VRN}&quot;);

if (phv_flag.equals(&quot;false&quot;)) 
{
   log.info(&quot;......................03 No Taxi or PHV question - ${VRN}&quot;);
}
else if (phv_flag.equals(&quot;true&quot;)) 
{
   log.info(&quot;.................... 03 Taxi or PHV Question appears - ${VRN}&quot;);
}
else if (taxi_row_flag == null)
{
   log.info(&quot;.................... 03 No Taxi row appears - ${VRN}&quot;);	
}
else if (taxi_row_flag != null)
{  
	log.info(&quot;.................... 03 Taxi row appears - ${VRN}&quot;);	
}
else
{
   log.info(&quot;.................... 03 EXEMPTED / Unrecognised - VRN: ${VRN} &quot;);
 	prev.setTestLogicalAction(org.apache.jmeter.threads.JMeterContext.TestLogicalAction.START_NEXT_ITERATION_OF_CURRENT_LOOP);
}</stringProp>
